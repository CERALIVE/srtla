{
  "name": "@ceralive/srtla",
  "version": "0.1.0",
  "description": "Type-safe helpers for srtla_send and srtla_rec (args, validation, process helpers).",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./sender": {
      "import": "./dist/sender/index.js",
      "types": "./dist/sender/index.d.ts"
    },
    "./receiver": {
